curl -X POST \
  http://$ORION_ENDPOINT:1026/v2/entities \
  -H 'Content-Type: application/json' \
  -d '{
    "id": "Room1",
    "type": "Room",
    "temperature": {
      "value": 23.5,
      "type": "Float"
    },
    "pressure": {
      "value": 720,
      "type": "Integer"
    }
  }'